workspace.status () {
echo "\
PATH=$PATH
RUNTIME=$RUNTIME
WORKSPACE=$WORKSPACE
$(env | grep ^PY)
$(env | grep ^GO)
$(env | grep ^JAVA)
$(env | grep ^MODULE)
$(env | grep ^GIT)
PROJECT=$PROJECT
CATALOG=$CATALOG
BRANCH=$(branch)
REV=$(rev)
VERSION=$(version)
NAME=$NAME
PWD=$(pwd)
VCS=$(vcs)
$(vcs.status)"
}
