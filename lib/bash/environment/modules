## REF: http://modules.sourceforge.net/
## REF: https://modules.readthedocs.io/en/latest/module.html
## upstream: https://github.com/cea-hpc/modules
type module || MODULES=missing return 
module avail && MODULES=available
MODULESHOME=$MODULESHOME
MODULES_CMD=$MODULES_CMD
MODULES_ML=$MODULES_ML
MODULES_VERBOSITY=$MODULES_VERBOSITY
MODULES_PAGER=$MODULES_PAGER
MODULEPATH=$MODULEPATH
MODULES_SEARCH_MATCH=$MODULES_SEARCH_MATCH
MODULES_UNLOAD_MATCH_ORDER=$MODULES_UNLOAD_MATCH_ORDER
LOADEDMODULES=$LOADEDMODULES
MODULERCFILE=$MODULERCFILE
MODULES_ADVANCED_VERSION_SPEC=$MODULES_ADVANCED_VERSION_SPEC
MODULES_AUTO_HANDLING=$MODULES_AUTO_HANDLING
MODULES_AVAIL_INDEPTH=$MODULES_AVAIL_INDEPTH
MODULES_COLLECTION_PIN_VERSION=$MODULES_COLLECTION_PIN_VERSION
MODULES_COLLECTION_TARGET=$MODULES_COLLECTION_TARGET
MODULES_COLOR=$MODULES_COLOR
MODULES_COLORS=$MODULES_COLORS
MODULES_TERM_BACKGROUND=$MODULES_TERM_BACKGROUND
MODULES_EXTENDED_DEFAULT=$MODULES_EXTENDED_DEFAULT
MODULES_ICASE=$MODULES_ICASE
MODULES_IMPLICIT_DEFAULT=$MODULES_IMPLICIT_DEFAULT
_LMFILES_=$_LMFILES_
MODULES_SET_SHELL_STARTUP=$MODULES_SET_SHELL_STARTUP
MODULES_SILENT_SHELL_DEBUG=1
cat /usr/share/Modules/etc/rc
stat $HOME/.modulerc
stat $HOME/.module
